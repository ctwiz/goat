<table>
  <thead>
    <tr>
      <td>ID</td>
      <td>Company</td>
      <td>Reporting Date</td>
      <td>Fiscal Quarter</td>
      <td>EPS forecast</td>
      <td>Last year's EPS</td>
      <td>Last Updated</td>
    </tr>
  </thead>
  <tbody>
    <% @earnings.each do |earning| %>
      <tr>
        <td><%=earning.id%></td>
        <td><%=earning.company.name%></td>
        <td><%=earning.reporting_date%></td>
        <td><%=earning.fiscal_quarter%></td>
        <td><%=earning.eps_forecast%></td>
        <td><%=earning.last_year_eps%></td>
        <td><%=earning.updated_at.in_time_zone("Pacific Time (US & Canada)")%></td>
      </tr>
    <% end %>
  </tbody>
</table>